<?xml version="1.0"?>
<launch>

  <!-- select a robot -->
  <arg name="robot_name" default="$(optenv ROBOT ur5_on_stand)" />

  <!-- full path to urdf model -->
  <arg name="urdf_path" default="$(find chargepal_description)/urdf/$(arg robot_name).urdf.xacro" />

  <include file="$(find chargepal_description)/launch/robot_specific_urdf/upload_$(arg robot_name)_urdf.launch">
    <arg name="urdf_path" value="$(arg urdf_path)" />
  </include>

</launch>
